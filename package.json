{
  "name": "browser-fingerprint",
  "version": "1.0.0",
  "description": "A website to show a example of Browser Fingerprinting.",
  "repository": "git@github.com:BrOrlandi/browser-fingerprint.git",
  "author": "Bruno Orlandi",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && parcel build ./src/index.html --public-url /browser-fingerprint --no-cache",
    "build-dev": "yarn build --public-url / --no-cache",
    "start": "parcel ./src/index.html -p 8080"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "parcel-bundler": "^1.9.7",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "fingerprintjs2": "^1.8.1"
  }
}
